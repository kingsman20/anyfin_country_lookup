type Query {
  countries(name: String): [Country!]!
  sayHello: String!
}

type Country {
  name: String!
  population: Int!
  currencies: [Currency!]!
}

type Currency {
  base: String!
  date: String!
  rates: [Rate!]!
}

type Rate {
  SEK: String!
}

type User {
  id: ID!
  email: String!
  token: String!
}

type Mutation {
  LoginUser(email: String!, password: String!): User!
}
